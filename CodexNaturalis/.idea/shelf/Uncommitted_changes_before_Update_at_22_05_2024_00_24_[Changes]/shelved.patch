        if (nonEmptyCols[k]) {\r\n                System.out.print(k + \" \");\r\n            }\r\n        }\r\n        System.out.print(\"\\n\");\r\n\r\n\r\n        for (int i = 0; i < Constants.MATRIXDIM; i++) {\r\n            if (nonEmptyRows[i]) {\r\n                System.out.print(i + \" \");\r\n                for (int j = 0; j < Constants.MATRIXDIM; j++) {\r\n                    if (nonEmptyCols[j]) {\r\n                        if (field.getCell(i, j, Constants.MATRIXDIM).isFilled()) {\r\n                            System.out.print(field.getCell(i, j, Constants.MATRIXDIM).getShort_value() + \" \");\r\n                        } else {\r\n                            System.out.print(\"   \");\r\n                        }\r\n                    }\r\n                }\r\n                System.out.print(\"\\n\");\r\n            }\r\n        }\r\n    }\r\n\r\n\r\n}\r\n
