+    public List<GameController> getFreeGames() throws RemoteException {
+        if( controllers.isEmpty() ) return null;
+        List<GameController> free = new ArrayList<>();
+        for ( GameController gc : controllers ){
+            if( !gc.getFull() ) free.add(gc);
+        }
+        return free;
     }
 
 
