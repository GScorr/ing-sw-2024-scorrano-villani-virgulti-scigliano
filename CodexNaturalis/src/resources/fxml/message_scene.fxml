<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.VIEW.GuiPackage.CONTROLLER.MessageSceneController">
    <Label fx:id="messageTitle" text="Alert Title" style="-fx-font-size: 16px; -fx-font-weight: bold;" />
    <Label fx:id="messageMessage" text="Alert message goes here" wrapText="true" style="-fx-font-size: 14px;" />
    <Button text="OK" onAction="#handleOkAction" style="-fx-font-size: 14px;" />
</VBox>
