<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="162.0" prefWidth="393.0" spacing="10" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.VIEW.GuiPackage.CONTROLLER.MessageSceneController">
    <Label fx:id="messageTitle" alignment="CENTER" prefHeight="34.0" prefWidth="103.0" style="-fx-font-size: 16px; -fx-font-weight: bold;" text="Alert Title">
      <font>
         <Font size="18.0" />
      </font></Label>
    <Label fx:id="messageMessage" alignment="CENTER" prefHeight="33.0" prefWidth="184.0" style="-fx-font-size: 14px;" text="Alert message goes here" wrapText="true">
      <font>
         <Font size="16.0" />
      </font></Label>
    <Button mnemonicParsing="false" onAction="#handleOkAction" prefHeight="33.0" prefWidth="61.0" style="-fx-font-size: 14px;" text="OK">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Button>
</VBox>
